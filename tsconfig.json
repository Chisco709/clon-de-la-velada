{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "./src/*"
      ],
      "@components/*": [
        "./src/components/*"
      ],
      "@layouts/*": [
        "./src/layouts/*"
      ],
      "@pages/*": [
        "./src/pages/*"
      ],
      "@sections/*": [
        "./src/sections/*"
      ],
      "@icons/*": [
        "./src/icons/*"
      ],
      "@consts/*": [
        "./src/consts/*"
      ],
    }
  }
}
